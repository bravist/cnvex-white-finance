{
    "name": "bravist/cnvex-white-finance",
    "description": "The cnvex.cn White Finance PHP SDK",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.3 || ^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0 || ^6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "bravist",
            "email": "chenghuiyong1987@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Bravist\\CnvexWhiteFinance\\": "src/"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bravist\\CnvexWhiteFinance\\ServiceProvider"
            ],
            "aliases": {
                "CnvexWhiteFinance": "\\Bravist\\CnvexWhiteFinance\\Facade\\CnvexWhiteFinance"
            }
        }
    }
